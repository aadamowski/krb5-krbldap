CFLAGS = $(CCOPTS) $(DEFS) $(LOCALINCLUDE)

PROG = kdb5_create
OBJS = kdb5_create.o kadm5_create.o string_table.o

all:: $(PROG)

$(PROG): $(OBJS) $(DEPLIBS)
	$(CC) $(LDFLAGS) $(LDARGS) -o $(PROG) $(OBJS) $(LIBS)

install::
	$(INSTALL_PROGRAM) $(PROG) ${DESTDIR}$(ADMIN_BINDIR)/$(PROG)

clean::
	$(RM) $(PROG) $(OBJS)
