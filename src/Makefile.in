	cd ..\..\include
	-$(MAKE) -$(MAKEFLAGS) 
	@echo Making in util\et
	cd ..\util\et
	-$(MAKE) -$(MAKEFLAGS) 
	@echo Making in util\profile
	cd ..\profile
	-$(MAKE) -$(MAKEFLAGS) 
