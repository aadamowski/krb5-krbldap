!INCLUDE <WIN32.MAK>

CFLAGS = -I../include $(cdebug) $(cflags) $(cvarsdll)

CC_COMMON_OBJS = marshall.obj msg.obj generic_lists.obj

CC_COMMON_LIB = cc_common.lib

$(CC_COMMON_LIB): $(CC_COMMON_OBJS)
        $(implib) /NOLOGO /OUT:$@ $**

all: $(CC_COMMON_LIB)

clean:
        del *.obj *.lib
