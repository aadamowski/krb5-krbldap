BUILDTOP=./../../$(top_dir)
SRCTOP=$(top_srcdir)/../..

CFLAGS = $(CCOPTS) $(DEFS)

##DOSBUILDTOP = ..\..\..
##DOSLIBNAME=..\crypto.lib

LIBOBJS = @LIBOBJS@

.c.o:
	$(CC) $(CFLAGS) -c $(srcdir)/$*.c
@SHARED_RULE@

COBJS=	rnd_confoun.$(OBJEXT) c_localaddr.$(OBJEXT) c_ustime.$(OBJEXT) 
OBJS= $(COBJS) $(LIBOBJS)

SRCS=	rnd_confoun.c c_localaddr.c c_ustime.c

all-unix:: shared $(OBJS) DONE

DONE: $(OBJS)
	@echo $(OBJS) > DONE

all-mac:: $(OBJS)

all-windows: $(COBJS)

shared:
	mkdir shared
	
memmove.o: $(SRCTOP)$(S)lib$(S)krb5$(S)posix$(S)memmove.c
	$(CC) $(CFLAGS) -c $*.c
@SHARED_RULE_LOCAL@


clean:: 
	$(RM) memmove.c

clean-unix::
	$(RM) shared/* DONE
clean-mac::
	$(RM) shared/*
clean-windows::
