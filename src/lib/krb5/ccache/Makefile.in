CFLAGS = $(CCOPTS) $(DEFS) -I$(srcdir)$(S)file -I$(srcdir)$(S)stdio

##DOSBUILDTOP = ..\..\..
##DOSLIBNAME=..\krb5.lib

MAC_SUBDIRS = file stdio

STLIBOBJS= \
	ccbase.o \
	ccdefault.o \
	ccdefops.o \
	ser_cc.o

OBJS=	ccbase.$(OBJEXT) \
	ccdefault.$(OBJEXT) \
	ccdefops.$(OBJEXT) \
	ser_cc.$(OBJEXT)

SRCS=	$(srcdir)/ccbase.c \
	$(srcdir)/ccdefault.c \
	$(srcdir)/ccdefops.c \
	$(srcdir)/ser_cc.c

all-unix:: all-libobjs

all-windows:: $(OBJS) 
	cd FILE
	@echo Making in ccache\file
	-$(MAKE) -$(MFLAGS)
	cd ..

clean-unix:: clean-libobjs

clean-windows::
	cd FILE
	@echo Making clean in ccache\file
	-$(MAKE) -$(MFLAGS) clean
	cd ..
	@echo Making clean locally
