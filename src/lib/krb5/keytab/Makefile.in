CFLAGS = $(CCOPTS) $(DEFS)

##DOSBUILDTOP = ..\..\..
##DOSLIBNAME=..\krb5.lib

MAC_SUBDIRS = file

STLIBOBJS= \
	ktadd.o		\
	ktbase.o	\
	ktdefault.o	\
	ktfr_entry.o	\
	ktremove.o	\
	read_servi.o

OBJS=	\
	ktadd.$(OBJEXT)		\
	ktbase.$(OBJEXT)	\
	ktdefault.$(OBJEXT)	\
	ktfr_entry.$(OBJEXT)	\
	ktremove.$(OBJEXT)	\
	read_servi.$(OBJEXT)

SRCS=	\
	$(srcdir)/ktadd.c	\
	$(srcdir)/ktbase.c	\
	$(srcdir)/ktdefault.c	\
	$(srcdir)/ktfr_entry.c	\
	$(srcdir)/ktremove.c	\
	$(srcdir)/read_servi.c

all-unix:: all-libobjs
all-windows:: $(OBJS)
	cd file
	@echo Making in keytab\file
	-$(MAKE) -$(MFLAGS)
	cd ..

clean-unix:: clean-libobjs

clean-windows::
	cd file
	@echo Making clean in keytab\file
	-$(MAKE) -$(MFLAGS) clean
	cd ..
	@echo Making clean locally

