BUILDTOP=./../../$(top_dir)
SRCTOP=$(top_srcdir)/../..

CFLAGS = $(CCOPTS) $(DEFS)

##DOSBUILDTOP = ..\..\..
##DOSLIBNAME=..\krb5.lib

.c.o:
	$(CC) $(CFLAGS) -c $(srcdir)/$*.c
@SHARED_RULE@

MACSRCS = getuid.c strcasecmp.c strdup.c

OBJS = setenv.$(OBJEXT)	@LIBOBJS@

all-unix:: shared $(OBJS) DONE
DONE: $(OBJS)
	@if test -n "$(OBJS)"; then echo $(OBJS) > DONE; fi

all-mac: $(OBJS)
all-windows:: syslog.obj sscanf.obj

shared:
	mkdir shared

clean-unix::
	$(RM) shared/*

clean-mac::
clean-windows::


